<Page x:Class="Poke.DexPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="Dex">
    <Grid>
        <StackPanel>
            <Button Content="Natures"
                    Click="GoToNatures"
                    Margin="5"/>
            <Button Content="Abilities"
                    Click="GoToAbilities"
                    Margin="5"/>
            <Button Content="Moves"
                    Click="GoToMoves"
                    Margin="5"/>
            <Button Content="Type Effectiveness"
                    Click="GoToTypeEffectiveness"
                    Margin="5"/>
        </StackPanel>
    </Grid>
</Page>
