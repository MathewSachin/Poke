<Window x:Class="Poke.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Poke" Height="300" Width="300">
    <StackPanel VerticalAlignment="Center">
        <Button Content="Battle Simulator"
                Click="OpenBattleSimulator"
                Margin="5"/>
        <Button Content="Double Battle Simulator"
                Click="OpenDoubleSimulator"
                Margin="5"/>
        <Button Content="Triple Battle Simulator"
                Click="OpenTripleSimulator"
                Margin="5"/>
        <Button Content="Dex"
                Click="OpenDex"
                Margin="5"/>
    </StackPanel>
</Window>
